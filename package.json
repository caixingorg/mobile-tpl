{
  "name": "react-mobile-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:sit": "tsc && vite build --mode sit",
    "build:prod": "tsc && vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,less,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,less,json,md}\"",
    "type-check": "tsc --noEmit",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,less,scss,json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "ahooks": "^3.8.0",
    "antd-mobile": "^5.35.0",
    "axios": "^1.6.8",
    "lodash-es": "^4.17.21",
    "qs": "^6.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.0",
    "@commitlint/config-conventional": "^20.4.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.30",
    "@types/postcss-pxtorem": "^6.0.3",
    "@types/qs": "^6.9.14",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "amfe-flexible": "^2.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.1.7",
    "less": "^4.2.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.38",
    "postcss-pxtorem": "^6.1.0",
    "prettier": "^3.8.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-svgr": "^4.2.0"
  }
}
